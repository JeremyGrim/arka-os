{
    "name":  "@arka-labs/cli",
    "version":  "1.0.0-core",
    "description":  "ARKA CLI - Command line interface for ARKA_LABS ecosystem",
    "main":  "dist/main.js",
    "bin":  {
                "arka":  "./dist/main.js"
            },
    "scripts":  {
                    "build":  "tsc",
                    "dev":  "tsc --watch",
                    "test":  "vitest run",
                    "test:watch":  "vitest",
                    "test:coverage":  "vitest run --coverage",
                    "lint":  "tsc --noEmit",
                    "verify:socle":  "node scripts/verify-socle.js"
                },
    "keywords":  [
                     "arka",
                     "cli",
                     "arka-labs",
                     "socle"
                 ],
    "author":  "ARKA Labs",
    "license":  "ISC",
    "dependencies":  {
                         "axios":  "^1.12.2",
                         "chalk":  "^5.6.2",
                         "commander":  "^11.1.0",
                         "fs-extra":  "^11.3.2",
                         "uuid":  "^9.0.1",
                         "yaml":  "^2.8.1"
                     },
    "devDependencies":  {
                            "@types/fs-extra":  "^11.0.4",
                            "@types/node":  "^20.19.19",
                            "@types/uuid":  "^9.0.8",
                            "typescript":  "^5.9.3",
                            "vitest":  "^1.6.1"
                        }
}
